<form id='modalsources' class='modal round col6 margin3 pin-top top2 bottom2 fill-light'>
  <a class='pad1 quiet icon close submit pin-topright x dot'></a>
  <h3 class='row1 pad1 keyline-bottom'>Sources</h3>
  <div class='pin-top pin-bottom pad1y pad2x fill-darken0 scroll-styled top1'>
  <%
    print(_(history.source).chain()
      .sortBy(function(item) {
        return (item.name||'').toLowerCase();
      })
      .map(function(item) {
        return this.sourceitem({
          classes: 'js-adddata ' + (item.id === style.source ? 'proj-active' : ''),
          item: item
        });
      }.bind(this))
      .value().join(''));
  %>
  </div>
</form>
