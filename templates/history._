<nav class='joined'><!--
  --><a class='button icon plus submit' href='/new/style'>Style</a><!--
  --><a class='button icon plus submit' href='/new/source'>Source</a>
</nav>

<section><h2>Styles</h2></section>

<div class='grid clearfix'>
<% _(obj.style).chain()
  .sortBy(function(item) { return -1 * item.mtime })
  .each(function(item) { %>
<a class='cell' href='/style?id=<%=item.id%>'><span class='thumbed'>
  <span class='image' style='background-image:url("/thumb.png?id=<%=item.id%>&mtime=<%=item.mtime%>")'></span>
  <strong><%= item.name || 'Untitled' %></strong>
  <code><%= item.id.split('/').pop() %></code>
</span></a>
<% }); %>
</div>

<section><h2>Sources</h2></section>

<div class='grid clearfix'>
<% _(obj.source).chain()
  .sortBy(function(item) { return -1 * item.mtime })
  .each(function(item) { %>
<a class='cell' href='/source?id=<%=item.id%>'><span class='thumbed'>
  <span class='image' style='background-image:url("/thumb.png?id=<%=item.id%>&mtime=<%=item.mtime%>")'></span>
  <strong><%= item.name || 'Untitled' %></strong>
  <code><%= item.id.split('/').pop() %></code>
</span></a>
<% }); %>
</div>
